<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1366.0"
	stylesheets="@../css/application.css"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.AjoutAdherentController">
	<!-- TODO Add Nodes -->
	<children>
		<Pane fx:id="paneAU" layoutX="114.0" layoutY="114.0"
			prefHeight="156.0" prefWidth="1366.0"
