			</children>
		</Pane>
		<JFXTextField fx:id="CINInput" layoutX="121.0"
			layoutY="242.0" prefHeight="41.0" prefWidth="391.0"
			promptText="Carte d'identité nationale (CIN)" />
		<JFXTextField fx:id="NomInput" layoutX="644.0"
			layoutY="242.0" prefHeight="41.0" prefWidth="391.0"
			promptText="Nom Adhérent" />
		<JFXTextField fx:id="PrenomInput" layoutX="644.0"
			layoutY="322.0" prefHeight="41.0" prefWidth="391.0"
